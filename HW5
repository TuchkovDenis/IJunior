using System;

namespace IJunior
{
    internal class Program
    {
        static void Main(string[] args)
        {
            string name = "Tuchkov";
            string surname = "Denis";
            Console.WriteLine($"{name} {surname}");
            string change;
            change = name;
            name = surname;
            surname = change;
            Console.WriteLine($"{name} {surname}");
            Console.ReadKey();
        }
    }
}
